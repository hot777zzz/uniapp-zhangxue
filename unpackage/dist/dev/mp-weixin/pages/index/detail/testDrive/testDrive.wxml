<view class="test-drive-container"><form data-event-opts="{{[['submit',[['handleSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="form-item"><text class="label required">预约车型</text><input class="input" type="text" disabled="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','modelName','$event',[]],['formData']]]]]}}" value="{{formData.modelName}}" bindinput="__e"/></view><view class="form-item"><text class="label required">您的称呼</text><input class="input" type="text" placeholder="请输入您的称呼" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['formData']]]]]}}" value="{{formData.name}}" bindinput="__e"/></view><view class="form-item"><text class="label required">您的城市</text><picker class="picker" mode="selector" range="{{cityList}}" range-key="name" data-event-opts="{{[['change',[['handleCityChange',['$event']]]]]}}" bindchange="__e"><view class="picker-value">{{''+(formData.city?formData.city.name:'请选择城市')+''}}</view></picker></view><view class="form-item"><text class="label required">门店选择</text><picker class="picker" mode="selector" range="{{storeList}}" range-key="name" disabled="{{!formData.city}}" data-event-opts="{{[['change',[['handleStoreChange',['$event']]]]]}}" bindchange="__e"><view class="picker-value">{{''+(formData.store?formData.store.name:'请选择门店')+''}}</view></picker></view><view class="form-item"><text class="label required">意向时间</text><picker class="picker" mode="date" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['handleDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker-value">{{''+(formData.date||'请选择意向时间')+''}}</view></picker></view><view class="form-item"><text class="label required">联系方式</text><input class="input" type="number" placeholder="请输入手机号码" maxlength="11" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['formData']]]]]}}" value="{{formData.phone}}" bindinput="__e"/></view><button class="submit-btn" form-type="submit">提交预约</button></form></view>